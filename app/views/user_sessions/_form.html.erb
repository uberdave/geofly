
<%= render "shared/login" %>
<%= form_tag user_sessions_url,  :rel=>"external" , :method => :post do %>
  <div class="field">
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>
  
  <div class="field">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </div>

    


<%= submit_tag "Login" %>


  
<% end %>

