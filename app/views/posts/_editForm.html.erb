<%= form_for(@post, :html => {:data => {:ajax => 'false'}}) do |f| %>
<p>

  <div class="field">
    <%= f.label :lat %><br />
    
 <%= f.text_field :lat, :value => @post.lat %>
  </div>
  <div class="field">
    <%  f.label :lng %><br />
    <%= f.text_field :lng, :value => @post.lng %>
    <%# f.text_field :lng %>
    </div>
   
    
  
 <div class="field">
    <%= f.file_field :image %>
  </div>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_area :title %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>